{
  "name": "front-end-engineering",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "commit": "git-cz",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:silent": "vitest run --silent",
    "lint": "yarn run lint:type && yarn run lint:eslint:all:fix && yarn run lint:prettier:all:fix && yarn run lint:style:all:fix",
    "lint:type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:staged": "lint-staged",
    "lint:eslint": "npx eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --cache --cache-location .cache/.eslintcache --ignore-path .eslintignore",
    "lint:eslint:fix": "yarn run lint:eslint --fix",
    "lint:eslint:all": "yarn run lint:eslint .",
    "lint:eslint:all:fix": "yarn run lint:eslint:all --fix",
    "lint:prettier": "npx prettier --check --cache --cache-location .cache/.prettier-cache --ignore-unknown --ignore-path .prettierignore",
    "lint:prettier:fix": "yarn run lint:prettier --write",
    "lint:prettier:all": "yarn run lint:prettier .",
    "lint:prettier:all:fix": "yarn run lint:prettier:all --write",
    "lint:style": "npx stylelint --cache --cache-location .cache/.stylelintcache --ignore-path .stylelintignore",
    "lint:style:fix": "yarn lint:style --fix",
    "lint:style:all": "yarn run lint:style \"**/*.{css,scss,vue}\"",
    "lint:style:all:fix": "yarn run lint:style:all --fix",
    "clean": "yarn cache clean --all && rimraf node_modules .cache",
    "clean:build": "rimraf dist",
    "clean:eslint": "rimraf .cache/.eslintcache",
    "clean:prettier": "rimraf .cache/.prettier-cache",
    "clean:stylelint": "rimraf .cache/.stylelintcache",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/cz-commitlint": "^17.6.7",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-vue": "^9.15.1",
    "eslint-plugin-yushu": "^0.0.2",
    "husky": "^8.0.0",
    "inquirer": "8",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "stylelint": "^15.10.2",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.6"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
